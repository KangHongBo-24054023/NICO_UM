<robot name="testbot">

<gazebo>
  <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    <robotNamespace>/testbot</robotNamespace>
  </plugin>
</gazebo>

<!-- Links -->	
	<link name="world"/> <!-- Used for fixing robot to Gazebo 'base_link' -->

	<link name="base_link">
		<visual>
			<origin xyz="0 0 0.01" rpy="0 0 0"/>
			<geometry>
				<box size="0.2 0.2 0.01"/>
			</geometry>
			<material name="black">
				<color rgba="0.0 0.0 0.0 1.0"/>
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0.01" rpy="0 0 0"/>
			<geometry>
				<box size="0.2 0.2 0.01"/>
			</geometry>
		</collision>
		<inertial>
			<origin xyz="0 0 0.01" rpy="0 0 0"/>
			<mass value ="1"/>
			<inertia 
				ixx="1.0" ixy="0.0" ixz="0.0"
				iyy="1.0" iyz="0.0"
				izz="1.0"/>
		</inertial>
	</link>
	<gazebo reference="base_link">
		<material>Gazebo/DarkGrey</material>
	</gazebo>

	<link name="base_cyl">
		<visual>
			<origin xyz="0 0 0.0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.05" length="0.1"/>
			</geometry>
			<material name="white">
				<color rgba="1.0 1.0 1.0 1.0"/>
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0.0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.05" length="0.1"/>
			</geometry>
		</collision>
		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value ="1"/>
			<inertia 
				ixx="1.0" ixy="0.0" ixz="0.0"
				iyy="1.0" iyz="0.0"
				izz="1.0"/>
		</inertial>
	</link>
	<gazebo reference="base_cyl">
		<material>Gazebo/DarkGrey</material>
	</gazebo>

	<link name="first_link_cyl">
		<visual>
			<origin xyz="0 0 0.0" rpy="1.57079632679 0 0"/>
			<geometry>
				<cylinder radius="0.05" length="0.1"/>
			</geometry>
			<material name="red">
				<color rgba="1.0 0.0 0.0 1.0"/>
