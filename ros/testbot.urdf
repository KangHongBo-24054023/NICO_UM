	<link name="tcp">
		<visual>
			<origin xyz="0 0 0" rpy="0 1.57079632679 0"/>
			<geometry>
				<box size="0.05 0.05 0.05"/>
			</geometry>
			<material name="gray">
				<color rgba="0.5 0.5 0.5 1.0"/>
			</material>
		</visual>
		<collision>
			<origin xyz="0 0 0" rpy="0 1.57079632679 0"/>
